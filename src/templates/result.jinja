{% extends "layout" %}
{% block title %}{{ super() }} | {{ title }} {% endblock %}
{% block body %}
<p>Timeout: <b>{{ timeout }}</b> seconds</p>
<p>Letter: <b>{{ letter }}</b></p>
<p>Categories:
     <ul>
        {% for (category, _) in card %}
            <li>{{ category }}</li>
        {% endfor %}
    </ul>
</p>
<form action="/round" method="get">
  <label for="start">
    Start next round?
    <input type="submit" value="Start">
  </label>
</form>
{% endblock %}
